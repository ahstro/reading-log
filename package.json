  "dependencies": {},
  "devDependencies": {
    "chokidar-cli": "^1.2.0",
    "elm": "^0.18.0",
    "uglify-js": "^3.1.0"
  },
  "scripts": {
    "build:dev": "elm make app/Main.elm --warn --output build/app.js ; exit 0",
    "build:prod": "yarn build:dev && uglifyjs --mangle --compress --output build/app.js -- build/app.js",
    "watch": "chokidar 'app/**/*.elm' -c 'yarn build:dev'"
  }
}
